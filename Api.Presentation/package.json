{
    "name": "api",
    "type": "module",
    "main": "index.fs.js",
    "scripts": {
        "clean": "dotnet fable clean .. --yes && dotnet fable clean .. --lang typescript --yes",
        "build": "dotnet fable && wrangler types",
        "build:ts": "dotnet fable --lang typescript --noCache && wrangler types",
        "start": "wrangler dev --port 80",
        "build+start": "npm run build && npm run start",
        "deploy": "wrangler deploy"
    },
    "devDependencies": {
        "typescript": "^5.5.2",
        "wrangler": "^4.42.2"
    }
}
